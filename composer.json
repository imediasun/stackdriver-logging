{
  "name": "devmagellan/stackdriver-logging",
  "description": "Log to Google Stackdriver Logging using Monolog",
  "authors": [
    {
      "name": "Bas van Vliet",
      "email": "dev.magellan@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Devmagellan\\StackdriverLogging\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "require": {
    "google/cloud-logging": "^1.10",
    "monolog/monolog": "~2.1"
  },
  "require-dev": {
    "orchestra/testbench": "~3.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Devmagellan\\StackdriverLogging\\StackdriverLoggerServiceProvider"
      ]
    }
  }
}
